{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/interfaces/*": [
        "src/interfaces/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}